# Copy this file to .env and fill in your actual values
# cp docker-compose.prod.env.example .env
# nano .env

# Groq API Key - Get it from https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API Key - For embeddings
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key - For web search (get it from https://tavily.com/)
TAVILY_API_KEY=your_tavily_api_key_here

# Google Cloud Credentials - For Gmail API
# Get from https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Gmail Token JSON (optional - if set, will be written to gmail_token.json on startup)
# Format: {"token": "...", "refresh_token": "...", "token_uri": "...", "client_id": "...", "client_secret": "...", "scopes": ["..."]}
# Leave empty if you want to use OAuth flow to generate it
GMAIL_TOKEN_JSON=

# Optional: API Settings
API_BASE_URL=http://localhost:8090
API_VERSION=v1

# Optional: LLM Settings
DEFAULT_MODEL=llama-3.1-70b-versatile
DEFAULT_TEMPERATURE=0.1

# Optional: Application Settings
DEBUG_MODE=false
LOG_LEVEL=INFO

# Optional: CORS Settings
CORS_ORIGINS=*
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=*
CORS_ALLOW_HEADERS=*

